    "compile:sass": "node compilers/sass.js",
    "compile:lore": "node compilers/lore-index.js"
  },
  "dependencies": {
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "axios": "^1.2.1",
    "cookies-next": "^2.1.1",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "next": "13.0.6",
    "papaparse": "^5.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rfdc": "^1.3.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/papaparse": "^5.3.5",
    "@types/testing-library__jest-dom": "^5.14.5",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.56.2",
    "ts-jest": "^29.0.3",
    "ts-standard": "^12.0.1"
  },
  "jest": {
    "preset": "ts-jest",
